{
    "build": {
        "dockerfile": "Dockerfile"
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
                "ms-python.black-formatter"
            ],
            "settings": {
                "[python]": {
                    "defaultInterpreterPath": "/usr/local/bin/python3.12",
                    "editor.defaultFormatter": "ms-python.black-formatter"
                }
            }
        }
    }
}